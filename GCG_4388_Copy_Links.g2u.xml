        <string>			Function.Global.DocumentControl.CreateReference(v.Local.sQuote, 50,v.Local.sQuoteLinkID)</string>
        <string>			Function.Global.DocumentControl.CopyExt(v.Local.sRFQLinkID, v.Local.sQuoteLinkID, false, true, V.Ambient.PUser)</string>
        <string>			f.Intrinsic.UI.Msgbox("Documents have been transferred")</string>
