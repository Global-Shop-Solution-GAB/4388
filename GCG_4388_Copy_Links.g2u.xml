        <string>f.Intrinsic.Control.If(v.Caller.Hook,=,10710)</string>
        <string>	f.Global.Security.IsInGroup(v.Caller.User,"DOCT",v.Local.bgroup)</string>
        <string>	f.Intrinsic.Control.If(v.Local.bgroup,=,True)</string>
        <string>		'set the text of the script button</string>
        <string>		v.Passed.000201.Set("Doc Trans")</string>
        <string>	f.Intrinsic.Control.EndIf</string>
        <string>f.Intrinsic.Control.EndIf</string>
        <string />
        <string>f.Intrinsic.Control.If(v.Caller.Hook,=,10960)</string>
        <string>	f.Global.Security.IsInGroup(v.Caller.User,"DOCT",v.Local.bgroup)</string>
        <string>	f.Intrinsic.Control.If(v.Local.bgroup,=,True)</string>
        <string>		'set the quote and the RFQ number</string>
        <string>		v.Local.sQuote.Set(v.Passed.000003)</string>
